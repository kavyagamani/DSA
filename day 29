# Python program to find minimum element in a 
# sorted rotated array using linear search

def findMin(arr):
    res = arr[0]

    # Traverse over arr[] to find minimum element
    for i in range(1, len(arr)):
        res = min(res, arr[i])

    return res

if __name__ == "__main__":
    arr = [5, 6, 1, 2, 3, 4]
    print(findMin(arr))
