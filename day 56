#User function Template for python3
class Solution:
    def subarraySum(self, arr, target):
        # code here
        s , e = 0, 0
        
        
        curr = 0
        
        for  i in range(len(arr)):
            curr += arr[i]
           
           
            while curr > target and s <= i:
                    curr -= arr[s]
                    s+=1
                
            if curr == target:
                   return [ s + 1, i + 1]
                    
        return [-1]
